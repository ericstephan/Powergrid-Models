schema:CurrentLimitShape
    a sh:NodeShape cim:CurrentLimit;


    sh:property [
    sh:path cim:IdentifiedObject.mRID   ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;

    sh:property [
    sh:path cim:IdentifiedObject.name ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;

    sh:property [
    sh:path cim:OperationalLimit.OperationalLimitSet ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;

    sh:property [
    sh:path cim:OperationalLimit.OperationalLimitType  ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;

  sh:property [
    sh:path cim:CurrentLimit.value ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;
    
.
