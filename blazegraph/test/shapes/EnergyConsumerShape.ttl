@prefix dash: <http://datashapes.org/dash#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix cim: <http://iec.ch/TC57/CIM100#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

#
#  Shape file for IEEE123 Feeder Models
#  Developer: Eric Stephan (ericstephan) 
#  Pacific Northwest National Laboratory
#  October 20, 2020
#  Version 0.1
#

schema:EnergyConsumerShape
    a sh:NodeShape cim:EnergyConsumer ;

    sh:property [
    sh:path cim:IdentifiedObject.mRID  ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;

    sh:property [
    sh:path cim:IdentifiedObject.name ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;

   sh:property [
    sh:path cim:Equipment.EquipmentContainer ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;

   sh:property [
    sh:path cim:ConductingEquipment.BaseVoltage ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;


     sh:property [
    sh:path cim:EnergyConsumer.LoadResponse ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;




    sh:property [
    sh:path cim:EnergyConsumer.p ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;


    sh:property [
    sh:path cim:EnergyConsumer.q  ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;


    sh:property [
    sh:path cim:EnergyConsumer.customerCount  ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;



   sh:property [
    sh:path cim:EnergyConsumer.phaseConnection ;
    sh:nodeKind sh:IRI ;
    sh:in (cim:PhaseShuntConnectionKind.Y )
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;

    sh:property [
    sh:path cim:EnergyConsumer.grounded   ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
   ] ;


     sh:property [
    sh:path cim:PowerSystemResource.Location ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] ;



.
